<%- include('../Includes/begin.ejs') %>

<%- include('../Includes/parent_nav.ejs') %>

<div class="container-fluid">
        <section id="content-wrapper">
            <main role="main" class="pt-5 main">
                <div class="main-content">
                    <div class="header pb-4 pb-sm-8 pt-5 pt-lg-8 d-flex align-items-center"
                         style="height: 100px; background-size: cover; background-position: center top;">
                    </div>
                    <div class="container-fluid mt--7">
                        <div class="row">
                            <div class="col-xl-12 order-xl-1">
                                <div class="card bg-secondary shadow">
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead class="thead-dark">
                                                <tr>
                                                    <th scope="col">#</th>
                                                    <th scope="col">Subject</th>
                                                    <th scope="col">Subject type</th>
                                                    <th scope="col">Marks</th>
                                                </tr>
                                                </thead>
                                                <tbody id="studentTable">
                                                <% if(marks.length>0) { %>
                                                <% for(var i=0; i < marks.length; i++) { %>
                                                    <tr>
                                                        <th scope="row">
                                                            <%= i + 1 %>
                                                        </th>
                                                        <td>
                                                            <%=marks[i].subject %>
                                                        </td>
                                                        <td>
                                                            <%=marks[i].subject_type %>
                                                        </td>
                                                        <td>
                                                            <%= marks[i].test_marks %>
                                                        </td>
                                                    </tr>
                                                <% } %>
                                                <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </section>
</div>
</body>

</html>
